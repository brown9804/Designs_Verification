#******************************************************************************
#				TARGETS
#******************************************************************************
HERE = ./
B4_SRC = ./source/

#******************************************************************************
#				4 BITS
#******************************************************************************

qflow:
	qflow synthesize place route counter_b4.v
#******************************************************************************

.PHONY: sta
sta:
	qflow sta counter_b4

############ CLEAN FOR ALL
.PHONY: clean 
clean:
	rm -rf $(B4_SRC)*.par $(B4_SRC)*.sh  $(HERE)*.sh $(HERE)*.log ./layout/* ./synthesis/* $(B4_SRC)*.blif


#******************************************************************************

